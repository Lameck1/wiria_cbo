{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "src/app/router.tsx"
  ],
  "project": [
    "src/**/*.{ts,tsx}"
  ],
  "paths": {
    "@/*": [
      "./src/*"
    ],
    "@/shared/*": [
      "./src/shared/*"
    ],
    "@/features/*": [
      "./src/features/*"
    ],
    "@/pages/*": [
      "./src/pages/*"
    ],
    "@/app/*": [
      "./src/app/*"
    ]
  },
  "ignore": [
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "tests/**"
  ],
  "ignoreDependencies": []
}